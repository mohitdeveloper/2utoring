@section scripts{
    <script>
        var commission = '30.00';

        function calcEarnings() {
            var price = document.getElementById('price').value;
            price = price.split("£");
            var users = document.getElementById('users').value;
            var lessons = document.getElementById('lessons').value;
            if (users > 10) {
                $('#nuser').text('Maximum of 10 student per class!');
                $('#nuser').css('color', 'red');
                $('#users').val('');
            } else {
                $('#nuser').text('');
            }


            var oldPrice = document.getElementById('price').value;
            var splitTxt = oldPrice.split("£");
            if (splitTxt[1]) {
                document.getElementById('price').value = '£' + splitTxt[1];
            } else {
                document.getElementById('price').value = '£' + splitTxt[0];
            }
            var result = price[1] * users * lessons;
            //result = result * (1 - (commission / 100));
            //result = result * (1 - (commission / 100));
            //Lesson Price * Number of Participants * Lessons per week
            document.getElementById('earnings').value = '£' + result.toFixed(2);
        }

    </script>

}





