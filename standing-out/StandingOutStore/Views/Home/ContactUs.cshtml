@model StandingOut.Data.DTO.ContactUs

@{
    ViewData["Title"] = "Get In Touch | 2utoring.com | Help & Support";
}

@section meta{
    <meta content="We’re here to answer your questions, form new collaborations, develop new featuresand ultimately create the best education platform for you." name="description">
    <meta content="Get In Touch | 2utoring.com | Help &amp; Support" property="og:title">
    <meta content="We’re here to answer your questions, form new collaborations, develop new featuresand ultimately create the best education platform for you." property="og:description">
    <meta content="Get In Touch | 2utoring.com | Help &amp; Support" property="twitter:title">
    <meta content="We’re here to answer your questions, form new collaborations, develop new featuresand ultimately create the best education platform for you." property="twitter:description">
    <meta property="og:type" content="website">
    <meta content="summary_large_image" name="twitter:card">
}

