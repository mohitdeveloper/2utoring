                        </svg>
                    </div>
                    <a asp-area="Admin" asp-controller="Settings" asp-action="ChangePassword"
                       class="fw-700 fs-16 Divider__Line" style="color: #2F80ED">Change Password</a>
                </div>
                }
            }
            <div class="d-flex align-center pt-28 pvalid">

                <div class="Icon__Square">
                    <i class="fa fa-times fw-400 fs-24" style="color: #2F80ED" aria-hidden="true"></i>
