{
  "name": "Standing Out Screen Share Tool",
  "description":
    "An extension to allow you to use Twilio Video screen sharing.",
  "version": "0.1.4",
  "manifest_version": 2,
  "background": {
    "scripts": ["extension.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://*.2utoring.com/*"]
  },
  "permissions": ["desktopCapture"]
}
